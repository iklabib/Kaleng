{
  "uid": 1000,
  "gid": 1000,
  "time_limit": 1,
  "rlimits": [
    { "resource": "RLIMIT_CORE", "soft": 1, "hard": 1 },
    { "resource": "RLIMIT_CPU", "soft": 1, "hard": 2 },
  ],
  "namespaces": ["CGROUP", "UTS", "IPC", "MNT", "USER", "PID", "NET", "TIME", "INTO_CGROUP"],
  "cgroup": {
    "max_pids": 40,
    "max_memory": "64M",
    "max_descendants": 1,
    "cpu": {
      "weight": 100,
      "time": 12500,
      "period": 100000
    }
  },
  "landlock": {
    "tty": false,
    "shared": true,
    "tmp": true,
    "vm_info": false,
    "dns": false,
    "certs": false,
    "files": [
      "d:rx:/app",
      "d:r:/sys/fs/cgroup"
    ]
  },
  "seccomp": {
    "default_action": "allow",
    "syscalls": [
      {
        "action": "errno",
        "names": [
          "connect",
          "accept",
          "sendto",
          "recvfrom",
          "sendmsg",
          "recvmsg",
          "bind",
          "listen",
          "setrlimit",
          "chown",
          "chmod",
          "chroot",
          "ptrace",
          "setresuid",
          "setresgid",
          "setreuid",
          "setregid",
          "setuid",
          "setgid"
        ]
      }
    ]
  }
}
